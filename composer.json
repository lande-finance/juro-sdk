{
    "name": "hashstudio/juro-sdk",
    "description": "SDK for Juro API",
    "keywords": [
        "juro",
        "php",
        "juro-sdk"
    ],
    "homepage": "https://github.com/hashstudio/juro-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alex",
            "email": "alex.nastasin@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/http": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "../../vendor/phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Hashstudio\\JuroSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hashstudio\\JuroSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hashstudio\\JuroSdk\\JuroSdkServiceProvider"
            ],
            "aliases": {
                "JuroSdk": "Hashstudio\\JuroSdk\\JuroSdkFacade"
            }
        }
    }
}
